{
  "name": "ciro-server",
  "version": "1.0.0",
  "description": "Ciro API Server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --max-old-space-size=8192 dist/index.js",
    "build": "tsc",
    "build:staging": "tsc",
    "dev": "nodemon --exec 'node --max-old-space-size=8192 -r ts-node/register src/index.ts'",
    "start:dev": "NODE_ENV=development node --max-old-space-size=8192 -r ts-node/register src/index.ts",
    "migrate": "knex migrate:latest --knexfile src/config/knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile src/config/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/config/knexfile.ts",
    "seed": "knex seed:run --knexfile src/config/knexfile.ts",
    "seed:make": "knex seed:make --knexfile src/config/knexfile.ts"
  },
  "dependencies": {
    "@hubspot/api-client": "^8.9.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@prisma/client": "^6.5.0",
    "@qdrant/js-client-rest": "^1.2.2",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^9.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.6",
    "dotenv": "^16.4.7",
    "exponential-backoff": "^3.1.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "express-slow-down": "^1.6.0",
    "express-validator": "^7.0.1",
    "gpt-tokenizer": "^2.1.1",
    "helmet": "^6.1.5",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "lru-cache": "^7.18.3",
    "mammoth": "^1.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.0.0",
    "p-limit": "^3.1.0",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "pdf.js-extract": "^0.2.1",
    "pg": "^8.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.1",
    "snowflake-sdk": "^1.6.21",
    "socket.io": "^4.6.1",
    "tesseract.js": "^4.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "ws": "^8.13.0",
