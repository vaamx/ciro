{
  "name": "ciro-server",
  "version": "1.0.0",
  "description": "Ciro chat application server",
  "main": "dist/main.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/main.js",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --delay 2.5 --exec 'ts-node -r tsconfig-paths/register' src/main.ts",
    "dev:safe": "REDIS_DISABLED=true NO_BULL=true nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --delay 2.5 --exec 'ts-node -r tsconfig-paths/register' src/main.ts",
    "dev:simple": "SIMPLE_SERVER=true nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --delay 2.5 --exec 'ts-node -r tsconfig-paths/register' src/main.ts",
    "dev:minimal": "nodemon --watch 'minimal.ts' --delay 1 --exec 'ts-node' minimal.ts",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "clean": "find src -name \"*.js\" -delete",
    "test": "jest",
    "db:init": "ts-node -r tsconfig-paths/register src/infrastructure/database/init.ts",
    "migrate:legacy": "ts-node -r tsconfig-paths/register src/infrastructure/database/migrate.ts",
    "migrate": "ts-node -r tsconfig-paths/register src/infrastructure/database/run-migrations.ts",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:consolidate": "ts-node -r tsconfig-paths/register migrations/utils/consolidate-migrations.ts",
    "migrate:convert-js": "ts-node -r tsconfig-paths/register migrations/utils/migration-converter.ts",
    "reset-db": "ts-node -r tsconfig-paths/register src/infrastructure/database/reset.ts",
    "seed": "ts-node -r tsconfig-paths/register src/infrastructure/database/seed.ts",
    "start:no-bull": "REDIS_DISABLED=true NO_BULL=true node -r tsconfig-paths/register dist/main.js"
  },
  "_moduleAliases": {
    "@core": "dist/core",
    "@modules": "dist/modules",
    "@infrastructure": "dist/infrastructure",
    "@common": "dist/common",
    "@database": "dist/database",
    "@services": "dist/services",
    "@utils": "dist/utils",
    "@config": "dist/config",
    "@types": "dist/types",
    "@middleware": "dist/middleware"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.796.0",
    "@aws-sdk/s3-request-presigner": "^3.796.0",
    "@hubspot/api-client": "^12.0.1",
    "@nestjs/bull": "10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/serve-static": "4.0.1",
    "@prisma/client": "^6.5.0",
    "@qdrant/js-client-rest": "^1.13.0",
    "@sendgrid/mail": "^8.1.4",
    "@supabase/supabase-js": "^2.48.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bull": "4.10.1",
    "cheerio": "^1.0.0",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-random-string": "^5.0.0",
    "dotenv": "^16.4.5",
    "exponential-backoff": "^3.1.2",
    "express": "~4.18.1",
    "express-rate-limit": "^7.5.0",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.2.1",
    "gpt-tokenizer": "^2.8.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.28.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "pdf.js-extract": "^0.2.1",
    "pdfjs-dist": "^5.1.91",
    "pg": "^8.13.3",
    "sharp": "^0.33.5",
