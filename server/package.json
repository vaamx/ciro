    "@types/snowflake-sdk": "^1.6.24",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.18.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.9",
    "prisma": "^6.5.0",
    "tesseract.js": "^6.0.1",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
